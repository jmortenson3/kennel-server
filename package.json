{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run copy-certs && npm run build-ts",
    "build-ts": "tsc",
    "copy-certs": "cp *.cert dist/ && cp *.key dist/",
    "start": "node dist/src/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/src/app.js"
  },
  "author": "jmortenson",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "3.0.6",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "pg": "7.12.1"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/jsonwebtoken": "8.3.6",
    "@types/node": "13.1.8",
    "@types/pg": "7.14.1",
    "concurrently": "5.0.2",
    "nodemon": "1.19.3",
    "typescript": "3.7.5"
  }
}
